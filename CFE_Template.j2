{
    "class": "Cloud_Failover",
    "environment": "aws",
    "externalStorage": {
        "scopingTags": {
            "f5_cloud_failover_label": "{{label}}"
        }
    },
    "failoverAddresses": {
        "enabled": true,
        "scopingTags": {
            "f5_cloud_failover_label": "{{label}}"
        }
    },
    "failoverRoutes": {
        "enabled": false,
        "scopingTags": {
            "f5_cloud_failover_label": "{{label}}"
        },
        "scopingAddressRanges": [
            {
                "range": "10.0.0.0/24"
            }
        ],
        "defaultNextHopAddresses": {
            "discoveryType": "static",
	    "items": [
		"{{ext_self_bigip1}}",
		"{{ext_self_bigip2}}"
            ]

        }
    },
    "controls": {
      "class": "Controls",
      "logLevel": "{{log_level}}"
    }
}
